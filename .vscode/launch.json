	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "node <current file>",
			"program": "${file}"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "sls debug post-vote",
			"cwd": "${fileDirname}",
			"program": "${workspaceFolder}/server-functions/node_modules/.bin/serverless",
			"args": [
				"invoke",
				"local",
				"-f",
				"post-vote",
				"--data",
				"{ \"voter\": \"Mike!!!!\"}"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "sls debug handle-vote",
			"cwd": "${fileDirname}",
			"program": "serverless",
			"args": [
				"invoke",
				"local",
				"-f",
				"handle-votes",
				"--data",
				"{ \"voter\": \"Mike!!!!\"}"
			]
		}
	]
}